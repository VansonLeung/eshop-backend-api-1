{
  "name": "sequelize-rest-framework",
  "version": "1.0.0",
  "description": "A reusable framework for building REST APIs with Sequelize ORM, featuring automatic CRUD operations, association endpoints, query builders, and built-in authentication/authorization",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./api": "./src/api/index.js",
    "./models": "./src/models/index.js",
    "./auth": "./src/auth/index.js",
    "./plugins": "./src/plugins/index.js"
  },
  "keywords": [
    "sequelize",
    "rest",
    "api",
    "crud",
    "express",
    "framework",
    "orm",
    "authentication",
    "authorization",
    "rbac",
    "acl",
    "jwt",
    "user-management"
  ],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "express": "^5.0.0",
    "sequelize": "^6.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {}
}
